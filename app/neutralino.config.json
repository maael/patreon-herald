{
  "applicationId": "js.patreon.herald",
  "defaultMode": "window",
  "version": "1.0.0",
  "port": 0,
  "url": "/resources/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "storage.*",
    "window.*",
    "clipboard.*",
    "debug.log"
  ],
  "globalVariables": {
    "TS": "MDZ6dGY1amYwdzd1dDFzaWF1c3hnOHFnZmx2ZGJm",
    "TID": "aWQ0Zm9qbzQ1NWoxbjJybGR6ODgycmV0dHZ1aHl1",
    "APP_VERSION": "1.0.0"
  },
  "modes": {
    "window": {
      "title": "Patreon Herald",
      "width": 800,
      "height": 600,
      "minWidth": 700,
      "minHeight": 400,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": false,
      "icon": "/resources/icons/appIcon.png",
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": false
    },
    "browser": {
      "globalVariables": {
        "APP_VERSION": "1.0.0"
      },
      "nativeBlockList": [
        "filesystem.*"
      ]
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    }
  },
  "cli": {
    "binaryName": "patreon-herald",
    "resourcesPath": "/resources/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "4.7.0",
    "clientVersion": "3.3.0"
  }
}